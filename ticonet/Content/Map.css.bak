#map-canvas {
    width: 100%;
    height: calc(100%);
    /*position: absolute;*/
    display: block;
}

.student-info-window {
    padding: 0 0 0 20px;
}

.iw-tab-content {
    border-left: solid 1px #DDDDDD;
    border-right: solid 1px #DDDDDD;
    border-bottom: solid 1px #DDDDDD;
    padding: 5px;
    width: 300px;
}

.iw-bus-info {
    margin: 10px 5px 0 5px;
    font-size: 14pt;
    color: #222222;
}

.iw-bus-info-small {
    margin: -3px 10px;
    font-size: 8pt;
    color: #666666;
}

.tbl-family {
    border: none;
    font-size: 10pt;
    color: #999999;
}

#dSearchBar {
    background-color: #222222;
    color: #999999;
    height: 40px;
    width: 44px;
    position: absolute;
    z-index: 998;
    bottom: 45px;
    left: 20px;
    padding: 3px 5px;
    border-radius: 3px;
    /*-webkit-box-shadow: 0 0 3px 1px rgba(0,0,0,0.65);
    -moz-box-shadow: 0 0 3px 1px rgba(0,0,0,0.65);
    box-shadow: 0 0 3px 1px rgba(0,0,0,0.65);*/
}

#btSearch {
    font-size: 16pt;
    cursor: pointer;
    margin: 4px 2px 2px 7px;
    padding: 0;
}

#dSearchForm {
    display: inline-block;
    width: 0;
    overflow: hidden;
    opacity: 0;
    padding: 0;
}

    #dSearchForm input {
        width: 280px;
    }

.students-fly {
    position: absolute;
    right: 25px;
    top: 10px;
    /*gilad change 31-7-16 
    width: 600px;*/
    min-height: 20px;
    background-color: #222;
    color: #999;
    direction: ltr;
    /*-webkit-box-shadow: -1px 1px 3px 1px rgba(0,0,0,0.65);
    -moz-box-shadow: -1px 1px 3px 1px rgba(0,0,0,0.65);
    box-shadow: -1px 1px 3px 1px rgba(0,0,0,0.65);*/
}

    .students-fly .header {
        padding: 10px 2px;
        position: relative;
        width: 100%;
        direction: ltr;
    }

        .students-fly .header h2 {
            padding: 0;
            margin: 0 5px;
            font-size: 16pt;
        }

        .students-fly .header .toggle {
            position: absolute;
            font-size: 16pt;
            right: 10px;
            top: 7px;
            cursor: pointer;
        }

            .students-fly .header .toggle:hover {
                color: white;
            }

    .students-fly .content {
        display: none;
    }

.ui-search-table input {
    padding: 0 !important;
}

.contextmenu {
    visibility: hidden;
    background: #222;
    border: 1px solid #222;
    z-index: 10;
    position: relative;
    width: 140px;
    max-width: 140px;
}

    .contextmenu div {
        padding: 5px 5px 5px 15px;
    }

    .contextmenu a {
        cursor: pointer;
        color: #999;
        text-decoration: none;
    }

        .contextmenu a:hover {
            color: #ffffff;
            text-decoration: underline;
        }

.ui-dialog-titlebar-close {
    background: #222 url("images/ui-icons_555555_256x240.png") -96px -128px no-repeat;
    border: none;
    width: 18px !important;
}

    .ui-dialog-titlebar-close:hover {
        background: #222 url("images/ui-icons_ffffff_256x240.png") -96px -128px no-repeat;
    }

.message {
    padding-top: 10px;
    text-align: center;
}

.text {
    background-color: white;
}

.color-indicator {
    border: 1px solid #ffffff;
    width: 50px;
    height: 20px;
    display: inline-block;
}

.color-indicator-small {
    border: 1px solid #ffffff;
    width: 10px;
    height: 10px;
    display: inline-block;
    margin-left: 5px;
}

.row {
    margin-left: 0;
    margin-right: 0;
}

.header a {
    cursor: pointer;
    color: #999;
    text-decoration: none;
}

    .header a:hover {
        color: #ffffff;
    }

#frmEditLine input, #frmEditLine select {
    width: 150px;
}

#dAttachColor span, #dAttachMultiline span {
    margin-left: 5px;
    margin-right: 5px;
}

.lines-table {
    width: 100%;
    border-collapse: collapse;
}

    .lines-table td {
        padding: 0;
        text-align: center;
    }

#dlgSchedule h2 {
    margin-top: 0;
    font-size: 14pt;
}

#dlgSchedule table {
    margin-bottom: 10px;
}
